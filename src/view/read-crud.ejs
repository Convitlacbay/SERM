<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>read-crud</title>
</head>

<body>
    <style>
        table,
        th,
        td {
            border: 1px solid black;
        }
    </style>
    <table style="width:100%">
        <tr>
            <th>Email</th>
            <th>Fist Name</th>
            <th>Last Name</th>
            <th>Adress</th>
            <th>Action</th>
        </tr>

        <% for(let i=0; i < dataTable.length; i++) { %>
            <tr>
                <td>
                    <%= dataTable[i].email %>
                </td>
                <td>
                    <%= dataTable[i].firstName %>
                </td>
                <td>
                    <%= dataTable[i].lastName %>
                </td>
                <td>
                    <%= dataTable[i].address %>
                </td>
                <td>
                    <a href="/update-crud?id= <%= dataTable[i].id %>" type="button"
                        style="border: none; cursor: pointer;">Update</a>
                    <a type="button" style="border: none; cursor: pointer;">Delete</a>
                </td>
            </tr>
            <% } %>

    </table>
</body>

</html>